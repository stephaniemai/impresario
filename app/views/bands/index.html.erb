<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <h1>List of bands:</h1>
      <% @bands.each do |band| %>
        <div>
          <h2><%= link_to band.name, band_path(band) %></h2>
        </div>
      <% end %>
      <p><%= link_to "Add your band", new_band_path, class: "btn btn-primary" %></p>
    </div>
  </div>
</div>
